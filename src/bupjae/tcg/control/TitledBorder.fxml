<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root xmlns:fx="http://javafx.com/fxml" type="StackPane" styleClass="titledBorder">
    <stylesheets>
        <URL value="@TitledBorder.css"/>
    </stylesheets>
    <children>
        <Label fx:id="titleLabel" styleClass="titledLabel" StackPane.alignment="TOP_LEFT"/>
        <StackPane fx:id="innerPane" styleClass="titledInner" StackPane.alignment="CENTER"/>
    </children>
</fx:root>
